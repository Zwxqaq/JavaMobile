document.write("<table border='1'>")
for (var x=1;x<=9;x++) {
    document.write("<tr>");
    for (var y=1;y<=x;y++) {
        document.write("<td>"+ x +"*"+ y +"="+(x*y)+"</td>");
    }
    for (var z=1;z<=9-x;z++) {
        document.write("<td>&nbsp;</td>")
    }
    document.write("</tr>");
}
document.write("</table>");


<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>var</title>
  <script type="text/javascript" src="Test_1.js"></script>
</head>
<body>

</body>
</html>
