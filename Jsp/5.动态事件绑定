window.onload=function () {
    loadFun();
}
function loadFun(){
    alert("页面加载了！");
}
---------------------------------------------------------------
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>var</title>
  <script type="text/javascript" src="Test_1.js"></script>
</head>
<body>
  <button id="MyId">PUSH MY QCX</button>
</body>
</html>


window.onload=function () {
    var obj = document.getElementById("MyId");
    if(obj != undefined) {
        obj.addEventListener("click",function () {loadFun()},false);
    }
}
function loadFun(){
    alert("页面加载了！");
}
